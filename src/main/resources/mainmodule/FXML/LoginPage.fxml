<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="720.0" prefWidth="1280.0"  xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainmodule.View.LoginView">
    <Button layoutX="486.0" layoutY="362.0" onMouseClicked="#exitClick" styleClass="exitButton" text="Exit" textAlignment="CENTER" />
    <VBox fx:id="mainBox" alignment="CENTER" layoutX="69.0" layoutY="115.0" prefWidth="230.0" spacing="10">
        <VBox id="usernameBox" fx:id="usernameBox" alignment="CENTER" spacing="10">
            <Text text="USERNAME" textAlignment="CENTER" />
            <TextField fx:id="username" alignment="CENTER" onKeyReleased="#enterUsername" promptText="Username" />
        </VBox>
        <VBox id="passwordBox" fx:id="passwordBox" alignment="CENTER" spacing="10">
            <Text text="PASSWORD" textAlignment="CENTER" />
            <TextField fx:id="password" alignment="CENTER" onKeyReleased="#enterPassword" promptText="Password" />
        </VBox>
        <HBox fx:id="buttonBox" alignment="CENTER" spacing="10">
            <Button fx:id="loginButton" onMouseClicked="#loginClick" text="Login" />
        </HBox>
    </VBox>
    <Label layoutX="403.0" layoutY="14.0" prefHeight="46.0" prefWidth="100.0" text="Login Page" textAlignment="CENTER" />
    <ChoiceBox styleClass="choiceBox" fx:id="choiceBox" layoutX="95.0" layoutY="45.0" prefHeight="32.0" prefWidth="178.0" />
</AnchorPane>