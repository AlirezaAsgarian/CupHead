<?xml version="1.0" encoding="UTF-8" ?>
<configuration debug="true">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>
                %d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n
            </Pattern>
        </encoder>
    </appender>
    <logger name="ModuleAbstractClasses.ModuleAbstractClasses.Controllers.CollisionController.CollisionController" level="OFF"/>
    <logger name="BossBirdsTypeA.BossBirds.MiniBossBird" level="DEBUG"/>
   <root level="DEBUG">
       <appender-ref ref="STDOUT"/>
   </root>
</configuration>