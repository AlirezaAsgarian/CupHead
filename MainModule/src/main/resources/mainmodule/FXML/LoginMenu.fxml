<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/18" fx:controller="mainmodule.LoginMenu.LoginMenuController">
    <center>
        <VBox alignment="CENTER" maxWidth="-Infinity" prefHeight="262.0" prefWidth="200.0" spacing="5.0"
              BorderPane.alignment="CENTER">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome" wrappingWidth="338.7294921875">
                    <font>
                        <Font size="23.0"/>
                    </font>
                </Text>
                <HBox alignment="CENTER" prefHeight="38.0" prefWidth="453.0">
                    <children>
                        <Label prefHeight="16.0" prefWidth="90.0" text="username : "/>
                        <TextField fx:id="username"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="42.0" prefWidth="600.0">
                    <children>
                        <Label prefHeight="16.0" prefWidth="90.0" text="password :"/>
                        <TextField fx:id="password"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" maxWidth="-Infinity" prefHeight="42.0" prefWidth="259.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#Login" text="Enter"/>
                    </children>
                </HBox>
                <Text fx:id="Error" strokeType="OUTSIDE" strokeWidth="0.0" text="ERROR" visible="false"
                      wrappingWidth="330.7294921875"/>
            </children>
        </VBox>
    </center>
</BorderPane>
